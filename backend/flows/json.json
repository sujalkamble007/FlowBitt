                "type": "code",
                "value": "import json\nfrom langflow.custom import Component\nfrom langflow.io import MessageInput, Output\nfrom langflow.schema.message import Message\n\n\nclass JSONAgentComponent(Component):\n    display_name = \"JSON Agent\"\n    description = \"Parses JSON input and extracts fields like name, email, and intent.\"\n    icon = \"file-text\"\n    name = \"JSONAgent\"\n\n    inputs = [\n        MessageInput(\n            name=\"message\",\n            display_name=\"Message\",\n            info=\"Raw JSON input passed as a message.\",\n        ),\n    ]\n\n    outputs = [\n        Output(display_name=\"Parsed Output\", name=\"text\", method=\"parse_json\"),\n    ]\n\n    def parse_json(self) -> Message:\n        try:\n            parsed = json.loads(self.message.text)\n            name = parsed.get(\"name\", \"N/A\")\n            email = parsed.get(\"email\", \"N/A\")\n            intent = parsed.get(\"intent\", \"N/A\")\n\n            result = f\"Extracted Fields:\\nName: {name}\\nEmail: {email}\\nIntent: {intent}\"\n        except Exception as e:\n            result = f\"❌ Error parsing JSON: {str(e)}\"\n\n        return Message(text=result)\n"
              },
